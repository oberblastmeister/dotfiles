{
    "haskell spec module": {
        "prefix": "Spec",
        "body": [
            "module $1Spec (spec) where",
            "",
            "import Test.Hspec",
            "",
            "spec :: Spec",
            "spec = do",
            "  $0"
        ],
        "description": ""
    },
    "hspec describe": {
        "prefix": "describe",
        "body": [
            "describe \"${1:description}\" $ do",
            "  $0"
        ],
        "description": ""
    },
    "hspec it": {
        "prefix": "it",
        "body": [
            "it \"${1:description}\" $ do",
            "  $0"
        ],
        "description": ""
    },
    "core viewing options": {
        "prefix": "core options",
        "body": [
            "{-# OPTIONS_GHC -ddump-simpl",
            "-ddump-to-file",
            "-dsuppress-module-prefixes",
            "-dsuppress-coercions",
            "-dsuppress-idinfo #-}"
        ],
        "description": ""
    }
}
